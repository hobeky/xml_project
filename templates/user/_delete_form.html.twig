{% dump(user) %}
    <form method="post"
          action="{{ path('app_user.delete', {'id': user.xmlId}) }}"
          onsubmit="return confirm('Are you sure you want to delete this item?');">
        <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ user.xmlId) }}">
        <button class="btn btn-danger">Delete</button>
    </form>
